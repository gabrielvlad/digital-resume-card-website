{
  "name": "digital-resume-card-website",
  "private": true,
  "description": "busines-card-style-resume-website",
  "version": "1.0.0",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "eslint:recommended",
      "google"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "bootstrap": "^4.3.1",
    "emailjs-com": "^3.2.0",
    "framer-motion": "^5.3.0",
    "gatsby": "^4.1.6",
    "gatsby-cli": "^4.4.0",
    "gatsby-plugin-react-helmet": "^5.1.0",
    "gatsby-plugin-sass": "^5.1.1",
    "gatsby-plugin-sharp": "^4.1.4",
    "gatsby-remark-images": "^6.1.4",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-source-filesystem": "^4.1.3",
    "gatsby-transformer-remark": "^5.1.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-focus-lock": "^2.6.0",
    "react-helmet": "^6.1.0",
    "react-notifications-component": "^3.0.3",
    "react-reveal": "^1.2.2",
    "react-scroll": "^1.8.4",
    "sass": "^1.43.4",
    "typed.js": "^2.0.11",
    "web-vitals": "^2.1.2"
  },
  "devDependencies": {
    "@iconify/react": "^3.1.0",
    "prettier": "^2.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
